<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
  name="%pluginName"
  id="org.eclipse.jdt"
  version="3.0.0"
  provider-name="%providerName">
  
  <requires>
      <import plugin="org.eclipse.ui.intro"/>
  </requires>
   
  <extension point="org.eclipse.ui.intro.configExtension"> 
     <configExtension configId="org.eclipse.platform.introConfig">  
		<extensionContent path="overview/overview-links" style="css/overview.css" alt-style="css/swt.properties">
           <link
	               url="http://org.eclipse.ui.intro/showHelpTopic?id=/org.eclipse.jdt.doc.user/gettingStarted/qs-BasicTutorial.htm"
	               label="%intro.java.label"
	               id="java">
	            <text>%intro.java.text</text>
	         </link>
    	</extensionContent>
      </configExtension>
     
     <configExtension configId="org.eclipse.platform.introConfig">  
		<extensionContent path="tutorials" style="css/tutorials.css" alt-style="css/swt.properties">
        	 <div
	         	label="%intro.div.label"
	            id="java">
	            <link
	            	label="%intro.app.label"
	               	url="http://org.eclipse.ui.intro/openCheatSheet?id=org.eclipse.ui.cheatsheet.sample.HelloWorld"
	               	id="hello-world">
	            	<text>%intro.app.text</text>
	         	</link>
	         	<link
	               	label="%intro.applet.label"
	               	url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
	               	id="applet">
	               	<text>%intro.applet.text</text>
	         	</link>
	         	<link
	               	label="%intro.swt.label"
	               	url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
	               	id="swt">
	            	<text>%intro.swt.text</text>
	         	</link>
	         	<link
	               label="%intro.ant.label"
	               url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
	               id="ant">
	            	<text>%intro.ant.text</text>
	         	</link>
	         </div>
    	</extensionContent>
     </configExtension>
     
     <!-- Extension to the SDK Samples Page. -->
  	 <configExtension configId="org.eclipse.platform.introConfig">  
		<extensionContent path="samples/workbench" style="css/samples.css" alt-style="css/swt.properties">
        	 <link
	               url="http://org.eclipse.ui.intro/runAction?pluginId=org.eclipse.pde.ui&amp;class=org.eclipse.pde.ui.internal.samples.ShowSampleAction&amp;id=org.eclipse.sdk.samples.javaeditor"
	               label="%intro.editor.label"
	               id="java-editor">
	            	<text>%intro.editor.text</text>
	         </link>
    	</extensionContent>
     </configExtension>
   </extension>    
</plugin>