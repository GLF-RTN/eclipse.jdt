<?xml version="1.0" encoding="UTF-8" ?> 
<introContent>

	<!-- Extension to the SDK Tutorials Page. -->
        <extensionContent
               alt-style="css/swt.properties"
               style="css/tutorials.css"
               path="tutorials">
            <div
                  label="Java Development"
                  id="java">
               <link
                     url="http://org.eclipse.ui.intro/showStandby?partId=org.eclipse.ui.cheatsheets.standbyPart&amp;input=org.eclipse.jdt.helloworld"
                     label="Build a simple Java application"
                     id="hello-world">
                  <text>Create a small Java program that will print "Hello, World" on the console</text>
               </link>
               <link
                     url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
                     label="Build a Java applet"
                     id="applet">
                  <text>Create a Java applet that can be embedded in a web page</text>
               </link>
               <link
                     url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
                     label="Build a standalone SWT application"
                     id="swt">
                  <text>Create a SWT application that runs without the workbench</text>
               </link>
               <link
                     url="http://org.eclipse.ui.intro/showMessage?message=Action not implemented."
                     label="Create and integrate an Ant script"
                     id="ant">
                  <text>Create an Ant script that can be executed using External Tools support</text>
               </link>
            </div>
         </extensionContent>
	
</introContent>
